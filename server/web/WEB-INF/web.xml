<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <description>Enrutamiento entre los request http y los servles o los archivos estaticos</description>
    <display-name>routes</display-name>
    <servlet>
        <description>Encargado de iniciar y cerrar sesion</description>
        <servlet-name>Sesion</servlet-name>
        <servlet-class>co.edu.UNal.ArquitecturaDeSoftware.Bienestar.Vista.Cuenta.VisAutenticacion</servlet-class>
    </servlet>
    <servlet>
        <description>Encargado de crear usaurios</description>
        <servlet-name>Registro</servlet-name>
        <servlet-class>co.edu.UNal.ArquitecturaDeSoftware.Bienestar.Vista.Cuenta.VisRegistro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sesion</servlet-name>
        <url-pattern>/network/sesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registro</servlet-name>
        <url-pattern>/network/registro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
